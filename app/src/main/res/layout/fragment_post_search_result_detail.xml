<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Views.PostSearchResultDetailActivity.FragmentPostSearchResultDetail">

    <TextView
        android:id="@+id/txtSearchResultDetailHeader"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:background="#E6E9FF"
        android:fontFamily="sans-serif-black"
        android:gravity="center"
        android:text="İLAN SAHİBİNİN PROFİLİ"
        android:textColor="#5C5C5C"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/statik1"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:fontFamily="sans-serif-black"
        android:gravity="center_vertical"
        android:text="Şehir"
        android:textColor="#5C5C5C"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/txtSearchResultDetailHeader"
        app:layout_constraintTop_toBottomOf="@+id/txtSearchResultDetailHeader"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:fontFamily="sans-serif-black"
        android:gravity="center_horizontal|center_vertical"
        android:text=":"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/textView21"
        app:layout_constraintTop_toTopOf="@+id/statik1"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/txtCarOptionalInfo"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="30dp"
        android:gravity="center_vertical"
        android:paddingStart="10dp"
        android:text="Şehir"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/btnSearchResultDetailAddToWish"
        app:layout_constraintEnd_toEndOf="@+id/txtCarModel"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageViewCarPic"
        app:layout_constraintTop_toBottomOf="@+id/imageViewCarPic"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/txtSearchResultDetailCity"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:gravity="center_vertical"
        android:paddingStart="10dp"
        android:text="Şehir"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/txtSearchResultDetailHeader"
        app:layout_constraintStart_toEndOf="@+id/textView20"
        app:layout_constraintTop_toTopOf="@+id/textView20"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/txtCarYear"
        android:layout_width="100dp"
        android:layout_height="45dp"
        android:layout_marginTop="5dp"
        android:gravity="center_vertical"
        android:paddingStart="10dp"
        android:text="Şehir"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewCarPic"
        app:layout_constraintStart_toStartOf="@+id/txtCarModel"
        app:layout_constraintTop_toTopOf="@+id/txtCarColor"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/txtCarColor"
        android:layout_width="100dp"
        android:layout_height="45dp"
        android:layout_marginTop="5dp"
        android:gravity="center_vertical"
        android:paddingStart="10dp"
        android:text="Şehir"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewCarPic"
        app:layout_constraintEnd_toEndOf="@+id/txtCarModel"
        app:layout_constraintStart_toEndOf="@+id/txtCarYear"
        app:layout_constraintTop_toBottomOf="@+id/txtCarModel"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/txtCarBrand3"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginEnd="20dp"
        android:gravity="center_vertical"
        android:paddingStart="10dp"
        android:text="Şehir"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/txtSearchResultDetailHeader"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/statik5"
        app:layout_constraintTop_toTopOf="@+id/imageViewCarPic"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/txtCarModel"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="20dp"
        android:gravity="center_vertical"
        android:paddingStart="10dp"
        android:text="Şehir"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/txtSearchResultDetailHeader"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/statik5"
        app:layout_constraintTop_toBottomOf="@+id/txtCarBrand3"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/statik2"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_marginTop="10dp"
        android:fontFamily="sans-serif-black"
        android:gravity="center_vertical"
        android:text="Boş Koltuk"
        android:textColor="#5C5C5C"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/statik1"
        app:layout_constraintTop_toBottomOf="@+id/statik1"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:fontFamily="sans-serif-black"
        android:gravity="center_horizontal|center_vertical"
        android:text=":"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/textView22"
        app:layout_constraintTop_toTopOf="@+id/statik2"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/txtSearchResultDetailPasangerCount"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginStart="20dp"
        android:gravity="center_vertical"
        android:paddingStart="10dp"
        android:text="Boş Koltuk"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/txtCarOptionalInfo"
        app:layout_constraintStart_toEndOf="@+id/textView21"
        app:layout_constraintTop_toTopOf="@+id/textView21"
        tools:ignore="HardcodedText,RtlSymmetry" />

    <TextView
        android:id="@+id/statik3"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_marginTop="10dp"
        android:fontFamily="sans-serif-black"
        android:gravity="center_vertical"
        android:text="Zaman"
        android:textColor="#5C5C5C"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/statik2"
        app:layout_constraintTop_toBottomOf="@+id/statik2"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:fontFamily="sans-serif-black"
        android:gravity="center_horizontal|center_vertical"
        android:text=":"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/textView23"
        app:layout_constraintTop_toTopOf="@+id/statik3"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/txtSearchResultDetailDateTime"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginStart="20dp"
        android:gravity="center_vertical"
        android:paddingStart="10dp"
        android:text="Zaman"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/txtSearchResultDetailPasangerCount"
        app:layout_constraintStart_toEndOf="@+id/textView22"
        app:layout_constraintTop_toTopOf="@+id/textView22"
        tools:ignore="HardcodedText,RtlSymmetry" />

    <TextView
        android:id="@+id/statik4"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_marginTop="10dp"
        android:fontFamily="sans-serif-black"
        android:gravity="center_vertical"
        android:text="Açıklama"
        android:textColor="#5C5C5C"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/statik3"
        app:layout_constraintTop_toBottomOf="@+id/statik3"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_marginStart="150dp"
        android:fontFamily="sans-serif-black"
        android:gravity="center_horizontal|center_vertical"
        android:text=":"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/statik4"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/txtSearchResultDetailDescription"
        android:layout_width="0dp"
        android:layout_height="55dp"
        android:layout_marginStart="20dp"
        android:gravity="center_vertical"
        android:paddingStart="10dp"
        android:scrollbars="vertical"
        android:text="Açıklama"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView23"
        app:layout_constraintEnd_toEndOf="@+id/txtSearchResultDetailDateTime"
        app:layout_constraintStart_toEndOf="@+id/textView23"
        app:layout_constraintTop_toTopOf="@+id/textView23"
        tools:ignore="HardcodedText,RtlSymmetry" />

    <TextView
        android:id="@+id/statik5"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_marginTop="4dp"
        android:fontFamily="sans-serif-black"
        android:gravity="center_vertical"
        android:text="Araç Bilgisi"
        android:textColor="#5C5C5C"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/statik4"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btnSearchResultDetailSendRequest"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="25dp"
        android:background="@drawable/deneme_button"
        android:fontFamily="sans-serif-black"
        android:text="KAYIT OL"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnSearchResultDetailAddToWish"
        tools:ignore="HardcodedText" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnSearchResultDetailDeletePost"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="100dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/deneme_button"
        android:backgroundTint="@color/design_default_color_error"
        android:fontFamily="sans-serif-black"
        android:text="SİL"
        android:textColor="#ffff"
        android:textSize="16sp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnSearchResultDetailSendRequest"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btnSearchResultDetailAddToWish"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/deneme_button"
        android:fontFamily="sans-serif-black"
        android:text="Takip et"
        app:layout_constraintEnd_toStartOf="@+id/btnSearchResultDetailSendRequest"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btnSearchResultDetailSendRequest"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btnSearchResultDetailStartService"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="40dp"
        android:text="Yolculuğu Başlatabilirsiniz"
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="@+id/btnSearchResultDetailSendRequest"
        app:layout_constraintStart_toStartOf="@+id/statik5"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btnSearchResultDetailEndService"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Takip Sistemini Sonlandır."
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="@+id/btnSearchResultDetailSendRequest"
        app:layout_constraintStart_toStartOf="@+id/statik5"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btnSearchResultDetailTrackLocation"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="50dp"
        android:layout_marginBottom="70dp"
        android:text="İlan Sahibini İzleyin"
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="@+id/btnSearchResultDetailSendRequest"
        app:layout_constraintStart_toStartOf="@+id/statik5"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/imageViewCarPic"
        android:layout_width="140sp"
        android:layout_height="140sp"
        android:src="@drawable/car_blue"
        app:layout_constraintEnd_toStartOf="@+id/txtCarYear"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/statik5" />

</androidx.constraintlayout.widget.ConstraintLayout>