<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Views.PostSearchResultDetailActivity.FragmentPostSearchPostOwner">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/searchProfileConsLayoutContent"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible">

        <TextView
            android:id="@+id/postSearchPostOwnerBD"
            android:layout_width="0dp"
            android:layout_height="45dp"
            android:background="@drawable/spinner_shape"
            android:ems="10"
            android:gravity="center_vertical"
            android:paddingStart="10dp"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/postSearchPostOwnerName"
            app:layout_constraintStart_toStartOf="@+id/postSearchPostOwnerName"
            app:layout_constraintTop_toTopOf="@+id/textView28"
            tools:ignore="RtlSymmetry" />

        <ImageView
            android:id="@+id/postSearchPostOwnerImageView"
            android:layout_width="150sp"
            android:layout_height="150sp"
            android:layout_marginStart="131dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="130dp"
            android:contentDescription="@null"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            app:srcCompat="@drawable/ic_image_holder"
            tools:src="@tools:sample/avatars" />

        <TextView
            android:id="@+id/textView27"
            android:layout_width="0dp"
            android:layout_height="45dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="110dp"
            android:fontFamily="sans-serif-black"
            android:gravity="center_vertical"
            android:text="Adı Soyadı"
            android:textColor="#5C5C5C"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/postSearchPostOwnerImageView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/postSearchPostOwnerImageView"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/postSearchPostOwnerName"
            android:layout_width="0dp"
            android:layout_height="45dp"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="10dp"
            android:background="@drawable/spinner_shape"
            android:ems="10"
            android:gravity="center_vertical"
            android:paddingStart="10dp"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView27"
            app:layout_constraintTop_toTopOf="@+id/textView27"
            tools:ignore="RtlSymmetry" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="0dp"
            android:layout_height="45dp"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="4dp"
            android:background="#E6E9FF"
            android:fontFamily="sans-serif-black"
            android:gravity="center"
            android:text="İLAN SAHİBİNİN PROFİLİ"
            android:textColor="#5C5C5C"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/postSearchPostOwnerImageView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/postSearchPostOwnerGender"
            android:layout_width="0dp"
            android:layout_height="45dp"
            android:background="@drawable/spinner_shape"
            android:ems="10"
            android:gravity="center_vertical"
            android:paddingStart="10dp"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/postSearchPostOwnerBD"
            app:layout_constraintStart_toStartOf="@+id/postSearchPostOwnerBD"
            app:layout_constraintTop_toTopOf="@+id/textView33"
            tools:ignore="RtlSymmetry" />

        <TextView
            android:id="@+id/textView28"
            android:layout_width="0dp"
            android:layout_height="45dp"
            android:layout_marginTop="10dp"
            android:fontFamily="sans-serif-black"
            android:gravity="center_vertical"
            android:text="Doğum Tarihi"
            android:textColor="#5C5C5C"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/textView27"
            app:layout_constraintStart_toStartOf="@+id/textView27"
            app:layout_constraintTop_toBottomOf="@+id/textView27"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/textView33"
            android:layout_width="0dp"
            android:layout_height="45dp"
            android:layout_marginTop="10dp"
            android:fontFamily="sans-serif-black"
            android:gravity="center_vertical"
            android:text="Cinsiyeti"
            android:textColor="#5C5C5C"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/textView28"
            app:layout_constraintStart_toStartOf="@+id/textView28"
            app:layout_constraintTop_toBottomOf="@+id/textView28"
            tools:ignore="HardcodedText" />

        <Button
            android:id="@+id/btnBlockUserProfile"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_marginStart="149dp"
            android:layout_marginEnd="150dp"
            android:layout_marginBottom="25dp"
            android:text="Engelle"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="HardcodedText" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/searchProfileConsLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ProgressBar
            android:id="@+id/searchResultProgress"
            android:indeterminate="true"
            android:indeterminateDrawable="@drawable/progressbar_style"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="50dp"
            android:layout_marginTop="150dp"
            android:layout_marginEnd="50dp"
            android:layout_marginBottom="150dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>