<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/textView12"
        android:layout_width="9dp"
        android:layout_height="18dp"
        android:layout_marginStart="12dp"
        android:text=":"
        android:textAlignment="center"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/textView9" />

    <TextView
        android:id="@+id/txtRCLPostHeaderRow"
        android:layout_width="315dp"
        android:layout_height="30dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="32dp"
        android:inputType="textMultiLine"
        android:text="TextView"
        android:textAlignment="center"
        android:textColor="#6C6B6B"
        android:textSize="18sp"
        android:textStyle="italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt2323"
        android:layout_width="108dp"
        android:layout_height="18dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="16dp"
        android:fontFamily="sans-serif"
        android:text="Tarih ve Saat"
        android:textAlignment="inherit"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtRCLPostHeaderRow" />

    <TextView
        android:id="@+id/txtRCLPostTimeRow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:text="TextView"
        android:textColor="#6C6B6B"
        android:textStyle="italic"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toTopOf="@+id/textView10" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="112dp"
        android:layout_height="18dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="8dp"
        android:text="Boş Koltuk Sayısı"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt2323" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="9dp"
        android:layout_height="18dp"
        android:layout_marginStart="16dp"
        android:text=":"
        android:textAlignment="center"
        app:layout_constraintStart_toEndOf="@+id/txt2323"
        app:layout_constraintTop_toTopOf="@+id/txt2323" />

    <TextView
        android:id="@+id/txtRCLPostPCROW"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:text="3"
        android:textColor="#6C6B6B"
        android:textStyle="italic"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toTopOf="@+id/textView12" />

    <View
        android:id="@+id/divider"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:layout_marginTop="16dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />
</androidx.constraintlayout.widget.ConstraintLayout>