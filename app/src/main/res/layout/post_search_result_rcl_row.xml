<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="5dp"
    android:layout_marginTop="5dp"
    android:layout_marginRight="5dp"
    android:layout_marginBottom="5dp"
    android:background="@drawable/spinner_shape">

    <TextView
        android:id="@+id/textView12"
        android:layout_width="9dp"
        android:layout_height="18dp"
        android:layout_marginStart="10dp"
        android:text=":"
        android:textAlignment="center"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/textView9"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/txtRCLPostHeaderRow"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:gravity="center"
        android:inputType="textShortMessage"
        android:text="TextView"
        android:textAlignment="center"
        android:textColor="#6C6B6B"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText,TextViewEdits" />

    <TextView
        android:id="@+id/txt2323"
        android:layout_width="0dp"
        android:layout_height="18dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="10dp"
        android:fontFamily="sans-serif"
        android:text="Tarih ve Saat"
        android:textAlignment="inherit"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/txtRCLPostHeaderRow"
        app:layout_constraintTop_toBottomOf="@+id/txtRCLPostHeaderRow"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/txtRCLPostTimeRow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:text="TextView"
        android:textColor="#6C6B6B"
        android:textStyle="italic"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toTopOf="@+id/textView10"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="18dp"
        android:layout_marginTop="5dp"
        android:text="Boş Koltuk Sayısı"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/txt2323"
        app:layout_constraintTop_toBottomOf="@+id/txt2323"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="9dp"
        android:layout_height="18dp"
        android:layout_marginStart="10dp"
        android:text=":"
        android:textAlignment="center"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/txt2323"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/txtRCLPostPCROW"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:text="3"
        android:textColor="#6C6B6B"
        android:textStyle="italic"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toTopOf="@+id/textView12"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btnRCLPostRemove"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="60dp"
        android:text="KALDIR"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtRCLPostPCROW"
        tools:ignore="HardcodedText" />
</androidx.constraintlayout.widget.ConstraintLayout>